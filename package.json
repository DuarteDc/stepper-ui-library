{
  "name": "stepper-ui",
  "version": "1.2.3",
  "description": "A customizable stepper component library for React with TailwindCSS.",
  "author": "DuarteBv",
  "keywords": [
    "react",
    "stepper",
    "ui",
    "tailwind",
    "component",
    "library",
    "typescript",
    "vite"
  ],
  "homepage": "https://portfolio-duartebv.netlify.app/demo/stepper-ui/",
  "license": "MIT",
  "main": "./dist/stepper-ui.umd.js",
  "module": "./dist/stepper-ui.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/stepper-ui.mjs",
      "require": "./dist/stepper-ui.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "vitest"
  },
  "peerDependencies": {
    "clsx": "^2.1.1",
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.14",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^9.0.0",
    "jest-dom": "link:@testing/library/jest-dom",
    "jsdom": "^27.0.0",
    "typescript": "^5.4.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.0.0",
    "vitest": "^3.2.4"
  }
}